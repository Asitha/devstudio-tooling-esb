<?xml version="1.0" encoding="UTF-8"?>
<proxy name="NumberCalculateService" startOnLoad="true" transports="http https" xmlns="http://ws.apache.org/ns/synapse">
    <target>
        <inSequence>
            <property description="Generate 202 response from mediation flow" name="FORCE_SC_ACCEPTED" scope="axis2" type="STRING" value="true"/>
            <header name="Action" scope="default" value="http://tempuri.org/SOAP.Demo.AddInteger"/>
            <store description="Publish request payload to JMS message store" messageStore="NumberCalculateMS"/>
        </inSequence>
        <outSequence/>
        <faultSequence/>
    </target>
</proxy>
